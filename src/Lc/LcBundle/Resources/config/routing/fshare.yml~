fshare:
    path:     /
    defaults: { _controller: "LcLcBundle:Fshare:index" }

fshare_show:
    path:     /{id}/show
    defaults: { _controller: "LcLcBundle:Fshare:show" }

fshare_new:
    path:     /new
    defaults: { _controller: "LcLcBundle:Fshare:new" }

fshare_create:
    path:     /create/{token}
    defaults: { _controller: "LcLcBundle:Fshare:create" }

fshare_edit:
    path:     /{id}/edit
    defaults: { _controller: "LcLcBundle:Fshare:edit" }

fshare_update:
    path:     /{id}/update
    defaults: { _controller: "LcLcBundle:Fshare:update" }
    requirements: { _method: post|put }

fshare_delete:
    path:     /{id}/delete
    defaults: { _controller: "LcLcBundle:Fshare:delete" }
    requirements: { _method: post|delete }
