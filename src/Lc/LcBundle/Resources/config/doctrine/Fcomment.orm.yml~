Lc\LcBundle\Entity\Fcomment:
 type: entity
 repositoryClass: Lc\LcBundle\Repository\FeelingRepository
 table: fcomment
 id:
  id:
   type: integer
   generator: { strategy: AUTO }
 fields:
  comment:
   type: text
  is_publish:
   type: boolean
  token: 
   type: string
   length: 255
   unique: true
  created_at:
   type: datetime
  updated_at:
   type: datetime
   nullable: true
 manyToOne:
  feeling:
   targetEntity: Feeling
   inversedBy: fcomment
   joinColumn:
    name: feeling_id
    referencedColumnName: id
  user:
   targetEntity: User
   inversedBy: fcomment
   joinColumn:
    name: user_id
    referencedColumnName: id
 lifecycleCallbacks:
  prePersist: [ setCreatedAtValue, setTokenValue ]
  preUpdate: [ setUpdatedAtValue ]
