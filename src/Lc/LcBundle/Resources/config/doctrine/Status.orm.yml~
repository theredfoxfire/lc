Lc\LcBundle\Entity\Status:
 type: entity
 table: status
 id:
  id:
   type: integer
   generator: { strategy: AUTO }
 fields:
  name:
   type: string
   length: 255
   unique: true
  is_active:
   type: boolean
  created_at:
   type: datetime
  updated_at:
   type: datetime
   nullable: true
  token:
   type: string
   length: 255
   unique: true
 oneToOne:
  usercriteria:
   targetEntity: Usercriteria
   mappedBy: status
  profile:
   targetEntity: Profile
   mappedBy: status
